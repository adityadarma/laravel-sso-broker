{
	"name": "adityadarma/laravel-sso-broker",
	"description": "Manage SSO client for laravel",
	"homepage": "https://github.com/adityadarma/laravel-sso-broker",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Aditya Darma",
			"email": "adhit.boys1@gmail.com"
		}
	],
	"require": {
		"php": "^8.0",
		"adityadarma/laravel-jwt-sso": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"AdityaDarma\\LaravelSsoBroker\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"AdityaDarma\\LaravelSsoBroker\\LaravelSsoBrokerServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true,
		"preferred-install": "dist",
		"optimize-autoloader": true
	},
	"minimum-stability": "stable"
}
